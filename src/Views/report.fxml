<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <AnchorPane prefHeight="700.0" prefWidth="900.0">
            <children>
                <Label fx:id="titleText" layoutX="375.0" layoutY="28.0" prefHeight="64.0" prefWidth="214.0" text="Report">
                    <font>
                        <Font name="Nadeem Regular" size="36.0" />
                    </font>
                </Label>
                <Button fx:id="logOff" layoutX="722.0" layoutY="657.0" mnemonicParsing="false" onAction="#onActionLogOff" prefHeight="25.0" prefWidth="160.0" text="Log off" />
                <Button layoutX="402.0" layoutY="606.0" mnemonicParsing="false" onAction="#onActionContactSubmit" prefHeight="27.0" prefWidth="88.0" text="submit" />
                <Button layoutX="655.0" layoutY="606.0" mnemonicParsing="false" onAction="#onActionCustomerSubmit" prefHeight="27.0" prefWidth="88.0" text="submit" />
            <TextArea fx:id="textArea" editable="false" layoutX="114.0" layoutY="110.0" prefHeight="443.0" prefWidth="663.0" />
            <ComboBox fx:id="typeList" layoutX="96.0" layoutY="593.0" prefHeight="27.0" prefWidth="88.0" promptText="Type" />
            <Label layoutX="376.0" layoutY="574.0" text="Contact Schedule reports" />
            <Label layoutX="640.0" layoutY="574.0" text="Customer List reports" />
            <Label layoutX="159.0" layoutY="574.0" text="Appointment reports" />
            <Button layoutX="215.0" layoutY="606.0" mnemonicParsing="false" onAction="#onActionAppointmentSubmit" prefHeight="27.0" prefWidth="88.0" text="submit" />
            <Button fx:id="back" layoutX="110.0" layoutY="656.0" mnemonicParsing="false" onAction="#onActionBack" prefHeight="25.0" prefWidth="160.0" text="Main menu" />
            <ComboBox fx:id="monthList" layoutX="96.0" layoutY="625.0" prefHeight="27.0" prefWidth="88.0" promptText="Month" />
            </children>
        </AnchorPane>
    </children>
</GridPane>
